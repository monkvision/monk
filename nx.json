{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "test"]
      }
    }
  },
  "targetDefaults": {
    "build:development": {
      "dependsOn": ["^build:development"],
      "outputs": ["{projectRoot}/dist"]
    },
    "build:staging": {
      "dependsOn": ["^build:staging"],
      "outputs": ["{projectRoot}/dist"]
    },
    "build:preview": {
      "dependsOn": ["^build:preview"],
      "outputs": ["{projectRoot}/dist"]
    },
    "build:backend-staging-qa": {
      "dependsOn": ["^build:backend-staging-qa"],
      "outputs": ["{projectRoot}/dist"]
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "serve": {
      "dependsOn": ["^serve"]
    },
    "deploy": {
      "dependsOn": ["^deploy"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "build:production": {
      "outputs": ["{projectRoot}/dist"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/lib", "{projectRoot}/dist"]
    }
  }
}
