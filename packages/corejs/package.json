{
  "name": "@monkvision/corejs",
  "author": "monkvision",
  "license": "BSD-3-Clause-Clear",
  "version": "1.0.7",
  "private": false,
  "description": "AI-powered vehicle damage detection for JavaScript",
  "homepage": "https://github.com/monkvision/monkjs/packages/corejs/#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monkvision/monkjs.git",
    "directory": "packages/corejs"
  },
  "bugs": {
    "url": "https://github.com/monkvision/monkjs/issues"
  },
  "keywords": [
    "javascript",
    "ai kit",
    "sdk",
    "vehicle damage detection"
  ],
  "main": "lib/index.js",
  "files": [
    "lib",
    "src",
    ".editorconfig",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "watch": "watch 'yarn run prepare' src",
    "ci": "mocha --require @babel/register --enable-source-maps --recursive ./test",
    "test": "yarn ci --watch --watch-extensions js ./test",
    "lint": "eslint src --cache",
    "prepare": "babel src --out-dir lib --source-maps",
    "release": "yarn lint && yarn build && release-it"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "*",
    "react": "^16.14.0 || ^17.0.0",
    "react-redux": "^7.2.1"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-redux": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/register": "^7.15.3",
    "@babel/runtime": "^7.15.4",
    "babel-plugin-inline-json-import": "^0.3.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-mocha": "^9.0.0",
    "core-js": "3",
    "mocha": "^9.1.3",
    "mocha-logger": "^1.0.7",
    "release-it": "^14.11.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
